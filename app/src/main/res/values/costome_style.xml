<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
        <item name="backgroundTint">@color/blue</item>
    </style>




    <!-- Costume Fonts-->
    <style name="small_font" parent="Theme.AppCompat.DayNight.NoActionBar">
         <item name="fontFamily">@font/nunito_regular</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="small_bold_font" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="fontFamily">@font/nunito_bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="mediam_font" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="fontFamily">@font/nunito_bold</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="regular_mediam_font" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="fontFamily">@font/nunito_regular</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>